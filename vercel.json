{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/tasks/two-step/experiment.html",
      "dest": "/public/tasks/two-step/experiment.html"
    },
    {
      "src": "/tasks/two-step/js/(.*)",
      "dest": "/public/tasks/two-step/js/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
